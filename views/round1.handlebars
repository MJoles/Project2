{{each movie}}
<div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
      <h1 class="display-5">Match 1</h1>
      <p class="lead">--------------------</p>
      <div class="container">
        <div class="card-deck mb-3 text-center">
          <div class="card mb-4 box-shadow">
            <div class="card-header">
              <h4 class="my-0 font-weight-normal">{{movie1.n}}<small class="text-muted"> (1)</small></h4>
            </div>
            <div class="card-body">
              <h1 class="card-title pricing-card-title">{{title}}</h1>
              <ul class="list-unstyled mt-3 mb-4">
                <li>---</li>
              </ul>
              <button type="button" class="btn btn-lg btn-block btn-primary">Vote!</button>
            </div>
          </div>
          <div class="card mb-4 box-shadow">
            <div class="card-header">
              <h4 class="my-0 font-weight-normal">{{movie2}}<small class="text-muted"> (2)</small></h4>
            </div>
            <div class="card-body">
              <h1 class="card-title pricing-card-title">{{}}</h1>
              <ul class="list-unstyled mt-3 mb-4">
                <li>---</li>
              </ul>
              <button type="button" class="btn btn-lg btn-block btn-primary">Vote!</button>
            </div>
          </div>
        </div>
      </div>
